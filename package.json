{
  "name": "kobalt",
  "version": "1.0.2",
  "description": "A cli to generate a theme from figma projects.",
  "main": "bin/main.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "start": "ts-node src/main.ts",
    "generate:types": "ts-node src/generate-types.ts",
    "generate:tokens": "ts-node src/modules/generate-tokens.ts",
    "prestart": "generate:types",
    "postinstall": "generate:types"
  },
  "preferGlobal": true,
  "bin": {
    "kobalt": "./bin/main.js"
  },
  "files": [
    "bin"
  ],
  "author": "Tim Alaerts",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node18-strictest": "^1.0.0",
    "@types/node": "^18.7.13",
    "dotenv": "^16.0.1",
    "quicktype": "^15.0.261",
    "style-dictionary": "^3.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "keywords": [
    "figma",
    "codegenerator",
    "react",
    "styled-components"
  ],
  "dependencies": {
    "axios": "^0.27.2"
  },
  "peerDependencies": {
    "@types/react": "^18.0.18",
    "@types/styled-components": "^5.1.26",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  }
}
